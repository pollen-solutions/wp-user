{
  "name": "pollen-solutions/wp-user",
  "description": "Pollen Solutions - Wordpress User Component - Abstraction lawer of WP_User object.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-user/",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "user",
    "role"
  ],
  "time": "2021-08-10",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.O",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpUser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpUser\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Solutions - Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
